{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Angular",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/src",
      "preLaunchTask": "npm: start",
      "sourceMaps": true
    },
    {
      "name": "Debug Angular Tests",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:9876/debug.html",
      "preLaunchTask": "npm: test",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    }
  ]
}
